Actor Catacomb_Step 
{
	+NOBLOCKMAP
	+CLIENTSIDEONLY
	+NOGRAVITY
	+NOINTERACTION
	+NOTIMEFREEZE
	Height 4
	Radius 2
	States
	{
		Spawn:
			TNT1 A 0
			TNT1 A 1 A_CheckFloor("Step")
			stop
		Step:
			TNT1 A 0 A_Jump(128, "StepRight")
			TNT1 A 0 A_PlaySound("Catacomb/FootStepLeft", CHAN_BODY, 1.0)
			stop
		StepRight:
			TNT1 A 1 A_PlaySound("Catacomb/FootStepRight", CHAN_BODY, 1.0)
			stop
	}
}

